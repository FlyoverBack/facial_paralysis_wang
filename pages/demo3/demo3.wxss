.container{
    width: 100%;
    height: 100%;
    display: flex;
    flex-direction: column;
    background: rgb(244,244,244);
}
.first{
    width: 100%;
    height: 100rpx;
    display: flex;
    justify-content: space-around;
    align-items: center;
    background: rgba(77, 129, 240, 0.2);
}
.search{
    width: 90%;
    border: 2rpx solid rgb(25, 88, 224);
    border-radius: 20rpx;
    display: flex;
    flex-direction: row;
    align-items: center;
    padding-left: 3%;
    background: #FFF;
}
icon{
    margin-right: 20rpx;
}

.second{
    width: 92%;
    display: flex;
    flex-direction: column;
    padding: 30rpx 4%;
    background: #FFF;
}
.content{
    width: 100%;
    display: flex;
    flex-direction: column;
}
.content view{
    font-size: 35rpx;
    text-align: center;
}
.content text{
    font-size: 25rpx;
    color: #333;
}